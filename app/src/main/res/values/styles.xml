<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowBackground">@drawable/loginsharp</item>
        <item name="android:fitsSystemWindows">true</item>
    </style>


    <style name="AppTheme.Launcher" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimaryDark">#9e9e9e</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowBackground">@drawable/launcher_sharp</item>
        <item name="android:fitsSystemWindows">true</item>
    </style>

    <style name="AppTheme.Float" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimaryDark">#9e9e9e</item>
        <item name="colorAccent">@android:color/white</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowBackground">@color/nothing</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:fitsSystemWindows">true</item>
    </style>



</resources>
